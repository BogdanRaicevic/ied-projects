{
  "name": "ied-be",
  "version": "1.0.0",
  "description": "READ ME stuff",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production tsx src/server.ts",
    "dev": "cross-env NODE_ENV=development tsx watch --inspect src/server.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "biome:format": "npx biome format --write ./src",
    "biome:check": "npx biome check --write ./src",
    "lint": "npx biome lint ./src",
    "lint:error": "npx biome lint --diagnostic-level error ./src",
    "generate:test-data": "tsx tests/utils/generateStaticData.ts"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=24.13.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@faker-js/faker": "^10.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.13",
    "@vitest/coverage-v8": "^4.0.18",
    "cross-env": "^10.1.0",
    "globals": "^17.3.0",
    "mongodb-memory-server": "^11.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/lib-storage": "^3.990.0",
    "@clerk/express": "^1.7.70",
    "cors": "^2.8.6",
    "cron": "^4.4.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "docxtemplater": "^3.68.1",
    "dotenv": "^17.3.1",
    "es-toolkit": "^1.44.0",
    "express": "^5.2.1",
    "ied-shared": "workspace:^",
    "mongoose": "^9.2.1",
    "node-cache": "^5.1.2",
    "pizzip": "^3.2.0",
    "zod": "^4.3.6"
  }
}
