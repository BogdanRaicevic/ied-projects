    "@types/express": "^5.0.6",
    "@types/node": "^22.19.3",
    "cross-env": "^10.1.0",
    "globals": "^16.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/lib-storage": "^3.958.0",
    "@clerk/express": "^1.7.60",
    "cors": "^2.8.5",
    "cron": "^4.4.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "docxtemplater": "^3.67.6",
    "dotenv": "^17.2.3",
    "es-toolkit": "^1.43.0",
    "express": "^5.2.1",
    "ied-shared": "workspace:^",
    "mongoose": "^8.18.2",
