{
  "name": "ied-shared",
  "version": "2.0.0",
  "description": "Shared types and utilities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "biome:format": "npx biome format --write ./src",
    "biome:check": "npx biome check --write ./src",
    "lint": "npx biome lint ./src",
    "lint:error": "npx biome lint --diagnostic-level error ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=24.13.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "@biomejs/biome": "^2.4.2"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "diff": "^8.0.3"
  }
}
